type Query {
	userById(id: ID!): UserLogin
	contentForWebAddress(webAddress: WebAddress!): [WebContent]!
}

type Mutation {
	login(credentials: NewUserLogin!): UserLogin
}

type UserLogin {
	id: ID!
	active: String!
	userId: String!
}

input WebAddress {
	endPoint: String!
}

type WebContent {
	id: ID!
	description: String!
	fileLocation: String!
}


input NewUserLogin {
	userId: String!
	password: String!
}

